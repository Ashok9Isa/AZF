{
    "connectorType": "massData",
    "connectorId": "randomImport",
    "connectorVersion": "0.1",
    "processingDirection": "inbound",
    "processingMode": "partial",
    "processors": [
        {
            "processorType": "inboundFactSheet",
            "processorName": "Random MassData BC",
            "processorDescription": "Imports random BusinessCapabilities for testing",
            "type": "BusinessCapability",
            "filter": {
                "type": "BusinessCapability"
            },
            "identifier": {
                "external": {
                    "id": {
                        "expr": "${content.id}"
                    },
                    "type": {
                        "expr": "externalId"
                    }
                }
            },
            "run": 0,
            "updates": [
                {
                    "key": {
                        "expr": "name"
                    },
                    "values": [
                        {
                            "expr": "${data.name}"
                        }
                    ]
                },
                {
                    "key": {
                        "expr": "description"
                    },
                    "values": [
                        {
                            "expr": "${data.name}"
                        }
                    ]
                }
            ],
            "enabled": true,
            "variables": [],
            "logLevel": "debug"
        },
        {
            "processorType": "inboundFactSheet",
            "processorName": "Random MassData App",
            "processorDescription": "Imports random Applications for testing",
            "type": "Application",
            "filter": {
                "type": "Application"
            },
            "identifier": {
                "external": {
                    "id": {
                        "expr": "${content.id}"
                    },
                    "type": {
                        "expr": "externalId"
                    }
                }
            },
            "run": 0,
            "updates": [
                {
                    "key": {
                        "expr": "name"
                    },
                    "values": [
                        {
                            "expr": "${data.name}"
                        }
                    ]
                },
                {
                    "key": {
                        "expr": "description"
                    },
                    "values": [
                        {
                            "expr": "${data.name}"
                        }
                    ]
                }
            ],
            "logLevel": "debug",
            "read": {
                "fields": [
                    "description"
                ],
                "relations": {
                    "filter": [
                        "relToParent"
                    ],
                    "fields": [
                        "activeFrom"
                    ],
                    "targetFields": [
                        "description"
                    ]
                },
                "tags": {
                    "groups": []
                },
                "subscriptions": {
                    "types": [
                        "RESPONSIBLE"
                    ]
                }
            }
        },
        {
            "processorType": "inboundRelation",
            "processorName": "Random MassData Rel from App to BC",
            "processorDescription": "Creates LeanIX Relations between the created Applications and BusinessCapabilities",
            "type": "relApplicationToBusinessCapability",
            "filter": {
                "type": "Application"
            },
            "from": {
                "external": {
                    "id": {
                        "expr": "${content.id}"
                    },
                    "type": {
                        "expr": "externalId"
                    }
                }
            },
            "to": {
                "external": {
                    "id": {
                        "expr": "${data.clusterName}"
                    },
                    "type": {
                        "expr": "externalId"
                    }
                }
            },
            "run": 1,
            "updates": [
                {
                    "key": {
                        "expr": "description"
                    },
                    "values": [
                        {
                            "expr": "Relationship Description"
                        }
                    ]
                }
            ],
            "logLevel": "debug"
        },
        {
            "processorType": "inboundRelation",
            "processorName": "Random MassData Rel from BC to BC",
            "processorDescription": "Creates LeanIX Relations between the created BusinessCapabilities and BusinessCapabilities",
            "type": "relToChild",
            "filter": {
                "type": "BusinessCapability"
            },
            "from": {
                "external": {
                    "id": {
                        "expr": "${data.parent}"
                    },
                    "type": {
                        "expr": "externalId"
                    }
                }
            },
            "to": {
                "external": {
                    "id": {
                        "expr": "${content.id}"
                    },
                    "type": {
                        "expr": "externalId"
                    }
                }
            },
            "run": 1,
            "updates": [
                {
                    "key": {
                        "expr": "description"
                    },
                    "values": [
                        {
                            "expr": "Hierarchy Description"
                        }
                    ]
                }
            ],
            "logLevel": "debug"
        }
    ],
    "variables": {
        "deploymentMaturity": {
            "default": "0"
        }
    }
}